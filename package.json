  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "start:trace": "cross-env NODE_OPTIONS='--trace-deprecation' electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "make:trace": "cross-env NODE_OPTIONS='--trace-deprecation' electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "prepublishOnly": "node scripts/update-patch-notes.js",
    "prepare:release": "node scripts/prepare-release.js",
    "publish-public": "python transfer_releases.py Drehon/vsAPP Drehon/vsAPP-public",
    "publish-public:win": ".venv\\Scripts\\activate && python transfer_releases.py Drehon/vsAPP Drehon/vsAPP-public",
    "publish-public:unix": "source .venv/bin/activate && python transfer_releases.py Drehon/vsAPP Drehon/vsAPP-public"
  },
  "keywords": [],
  "author": {
    "name": "Drehon",
    "email": "myemail@email.com"
  },
