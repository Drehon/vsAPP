    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "cross-env": "^10.0.0",
    "css-loader": "^7.1.2",
    "electron": "28.1.0",
    "eslint": "^9.33.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "globals": "^16.3.0",
    "jest": "^30.0.5",
    "@playwright/test": "^1.44.1",
    "playwright": "^1.44.1",
